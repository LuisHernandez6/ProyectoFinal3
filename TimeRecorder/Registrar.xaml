<Window x:Class="MonitorActividad.Registrar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MonitorActividad"
        mc:Ignorable="d"
        Title="Registrar" Height="379" Width="303">
    <Grid>
        <Label x:Name="label" Content="Nombre de Usuario:" HorizontalAlignment="Left" Margin="17,15,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Nombre:" HorizontalAlignment="Left" Margin="17,45,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Apellido:" HorizontalAlignment="Left" Margin="17,75,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label3" Content="Correo:" HorizontalAlignment="Left" Margin="17,105,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4" Content="Telefono:" HorizontalAlignment="Left" Margin="17,135,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label5" Content="Contraseña:" HorizontalAlignment="Left" Margin="17,165,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label6" Content="Confirmar contraseña:" HorizontalAlignment="Left" Margin="17,195,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="144,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="144,49,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="144,79,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Margin="144,109,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Margin="144,139,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="textBox5" HorizontalAlignment="Left" Margin="144,168,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="textBox6" HorizontalAlignment="Left" Margin="144,199,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Registrarse" HorizontalAlignment="Center" Margin="0,303,0,0" VerticalAlignment="Top" Click="button_Click" Width="124"/>
        <Label x:Name="label3_Copy" Content="Nivel:" HorizontalAlignment="Left" Margin="17,237,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4_Copy" Content="Perfil:" HorizontalAlignment="Left" Margin="17,267,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="61,241,0,0" VerticalAlignment="Top" Width="203" ItemsSource="{Binding Items}"/>
        <ComboBox x:Name="comboBox2" HorizontalAlignment="Left" Margin="61,268,0,0" VerticalAlignment="Top" Width="203" ItemsSource="{Binding Items1}"/>

    </Grid>
</Window>
